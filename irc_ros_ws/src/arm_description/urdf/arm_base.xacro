  <child link="waist_1"/>
  <axis xyz="0.0 0.0 1.0"/>
  <limit upper="${pi}" lower="${-pi}" effort="100" velocity="100"/>
</joint>

<joint name="shoulder_joint" type="revolute">
  <origin xyz="1e-06 0.0 0.0385" rpy="0 0 0"/>
